{
	"name": "lan-transfer",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"test:logger": "cross-env DEBUG=lan-transfer:* ts-node --project tsconfig.node.json src/examples/test-logger-colors.ts",
		"test:logger:win": "@powershell -Command $env:DEBUG='lan-transfer:*'; ts-node --project tsconfig.node.json src/examples/test-logger-colors.ts",
		"test:logger:js": "cross-env DEBUG=lan-transfer:* node src/examples/test-logger-colors.js",
		"test:logger:js:win": "@powershell -Command $env:DEBUG='lan-transfer:*'; node src/examples/test-logger-colors.js"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/debug": "^4.1.12",
		"@types/node": "^20.11.30",
		"@types/simple-peer": "^9.11.8",
		"cross-env": "^7.0.3",
		"ts-node": "^10.9.2",
		"typescript": "~5.7.2",
		"vite": "^6.2.0"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.0.12",
		"debug": "^4.4.0",
		"eruda": "^3.4.1",
		"loglevel": "^1.9.2",
		"parse-json": "^8.1.0",
		"simple-peer": "^9.11.1",
		"tailwindcss": "^4.0.12"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild"]
	}
}
